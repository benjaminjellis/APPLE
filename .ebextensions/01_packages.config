sh
commands:
  00_update_pip:
    command: "/opt/python/run/venv/bin/pip install --upgrade pip"
packages:
   yum:
      gcc-c++: []
      python36-devel: []
Resources:
  sslSecurityGroupIngress: 
    Type: AWS::EC2::SecurityGroupIngress
    Properties:
      GroupId: {"Fn::GetAtt" : ["AWSEBSecurityGroup", "GroupId"]}
      IpProtocol: tcp
      ToPort: 443
      FromPort: 443
      CidrIp: 0.0.0.0/0
